<?xml version="1.0" encoding="utf-8"?>
<openerp>
  <data>

    <template id="website_ehtml_options" name="eHTML Options" inherit_id="website.snippet_options">
        <xpath expr="//div" position="after">
            <div data-snippet-option-id='snippet_ehtml_options'
                data-selector=".twitter"
                data-selector-children=".oe_structure, [data-oe-type=html]">
            </div>
        </xpath>
    </template>

  </data>
</openerp>